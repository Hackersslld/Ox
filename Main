local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({
    Name = "OxyHub V2", 
    HidePremium = false, 
    SaveConfig = true, 
    ConfigFolder = "OxyHubConfig"
})

-- Subscription Status Tab
local SubTab = Window:MakeTab({
    Name = "Subscription Status",
    Icon = "rbxassetid://7743875759",  -- You can use a placeholder icon for the subscription tab
    PremiumOnly = false
})

-- Subscription data with end dates
local subscriptions = {
    [4057666714] = { type = "Owner", endDate = "Never" },
    [7100761159] = { type = "Monthly", endDate = "2024-08-31 23:59:59" },
    [7113890889] = { type = "Monthly", endDate = "2024-08-31 23:59:59" },
    [144210698]  = { type = "Monthly", endDate = "2024-08-31 23:59:59" },
    [1384102768] = { type = "Owner", endDate = "Never" }
}

-- Function to get the current date and time
local function getCurrentTime()
    return os.date("%Y-%m-%d %H:%M:%S", os.time())
end

-- Function to remove expired subscriptions
local function checkAndRemoveExpiredSubscriptions()
    local userId = game.Players.LocalPlayer.UserId
    local subscription = subscriptions[userId]
    if subscription and subscription.endDate ~= "Never" then
        if subscription.endDate <= getCurrentTime() then
            subscriptions[userId] = nil
            return "Subscription Expired"
        end
    end
end

-- Function to get subscription status
local function getSubscriptionStatus()
    local userId = game.Players.LocalPlayer.UserId
    local subscription = subscriptions[userId]
    if subscription then
        if subscription.type == "Owner" then
            return "Owner"
        else
            return subscription.type .. " - Ends on " .. subscription.endDate
        end
    else
        return "None"
    end
end

-- Perform the expiration check
checkAndRemoveExpiredSubscriptions()

-- Add status display to the Subscription Status tab
SubTab:AddLabel("Your Subscription Status:")
SubTab:AddLabel("Status: " .. getSubscriptionStatus())

-- Game Tabs (add these after the subscription check)
